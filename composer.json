{
    "name": "phpxmlrpc/polyfill-JSON",
    "description": "A pure-php reimplementation of the API exposed by the native JSON extension",
    "license": "BSD-3-Clause",
    "keywords": [ "json", "php", "extension", "polyfill" ],
    "require":
    {
        "php": "^5.3.0 || ^7.0 || ^8.0",
        "phpxmlrpc/jsonrpc": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^8.5.12",
        "yoast/phpunit-polyfills": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "PhpXmlRpc\\Polyfill\\Json\\": "src/"
        },
        "files": [
            "bootstrap.php"
        ]
    }
}
